# -*- mode: ruby -*-
# vi: set ft=ruby :

create_table "articles", force: :cascade do |t|
  t.string   "title"
  t.text     "text"
  t.datetime "created_at"
  t.datetime "updated_at"
end

create_table "monsters", force: :cascade do |t|
  t.string   "name"
  t.string  "type"
  t.string  "level"
  t.string  "attribute"
  t.datetime "created_at"
  t.datetime "updated_at"
end